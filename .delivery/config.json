{
  "version": "2",
  "build_cookbook": {
    "name": "build_cookbook",
    "path": ".delivery/build_cookbook"
  },
  "delivery-truck": {
    "lint": {
      "enable_cookstyle": true,
      "foodcritic": {
        "ignore_rules": ["FC048","FC069","FC078"],
        "only_rules": [],
        "excludes": [],
        "fail_tags": ["any"]
      }
    },
    "publish": {
      "chef_server": true
    }
  },
  "skip_phases": [
    "unit",
    "quality",
    "security",
    "smoke"
  ],
  "job_dispatch": {
    "version": "v2"
  },
  "dependencies": []
}
